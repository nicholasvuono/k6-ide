{
  "name": "k6-ide",
  "displayName": "",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.89.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "k6-ide",
          "title": "IDE k6",
          "icon": "src/k_logo_icon_247940.png"
        }
      ]
    },
    "views": {
      "k6-ide": [
        {
          "id": "testResults",
          "name": "View Test Results"
        }
      ]
    },
    "commands": [
      {
        "command": "k6-ide.viewTestResults",
        "title": "View Test Results - k6",
        "icon": "$(graph-line)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "k6-ide.viewTestResults",
          "when": "view == testResults",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
